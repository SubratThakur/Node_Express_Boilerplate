{
  "name": "notion_nodejs",
  "version": "1.0.0",
  "description": "Basic setup of NodeJS with typescript for Notion code design round",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest ./tests/**/*.test.js",
    "start": "tsc && node dist/index.js",
    "dev": "concurrently --kill-others \"npm:watch:*\"",
    "lint": "eslint --ext js,ts,tsx src/*",
    "watch:lint": "esw --watch --cache ./src/**/*.ts --fix",
    "watch:server": "APP_PORT=3001 nodemon --max-http-header-size=32768 -e js,ts --watch ./src/ --watch app.ts --watch index.ts --exec \"babel-node --extensions \".ts,.js\" ./index.ts\""
  },
  "author": "subrat.thakur",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.24.4",
    "@babel/node": "^7.23.9",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.17.12",
    "@babel/plugin-proposal-optional-chaining": "^7.17.12",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-typescript": "^7.17.12",
    "@babel/register": "^7.23.7",
    "@types/express": "^4.17.1",
    "@types/lodash": "^4.17.0",
    "@types/request": "^2.48.12",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "axios": "^1.6.8",
    "babel-loader": "^8.2.5",
    "babel-plugin-typescript-to-proptypes": "^2.0.0",
    "concurrently": "^7.2.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^16.1.4",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-jest-dom": "^3.9.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^5.0.0",
    "eslint-watch": "^7.0.0",
    "heap": "^0.2.7",
    "jest": "^29.7.0",
    "ts-loader": "^9.3.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "nodemon": "^3.1.0",
    "request": "^2.88.2"
  }
}
